<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<StackPane style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ChangeTemperature">
    <children>
        <HBox alignment="CENTER" maxWidth="1.7976931348623157E308">
            <children>
                <VBox alignment="CENTER_LEFT" prefHeight="389.0" prefWidth="314.0">
                    <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Change Temperature Limits">
                            <font>
                                <Font name="System Bold" size="24.0" />
                            </font>
                        </Text>
                        <Label prefHeight="30.0" prefWidth="78.0" text="Upper Limit">
                            <font>
                                <Font size="14.0" />
                            </font>
                            <VBox.margin>
                                <Insets top="30.0" />
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="upperLimit" style="-fx-border-width: 0 0 1 0; -fx-border-style: solid; -fx-background-color: white; -fx-border-insets: 0;">
                            <VBox.margin>
                                <Insets />
                            </VBox.margin>
                            <font>
                                <Font size="12.0" />
                            </font>
                        </TextField>
                        <Label text="Lower Limit">
                            <font>
                                <Font size="14.0" />
                            </font>
                            <VBox.margin>
                                <Insets top="30.0" />
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="lowermost"
                                   style="-fx-border-width: 0 0 1 0; -fx-border-style: solid; -fx-background-color: white; -fx-border-insets: 0;">
                            <font>
                                <Font size="12.0" />
                            </font>
                        </TextField>

                        <Button fx:id="register_btn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#save" style="-fx-background-color: white; -fx-border-style: solid;" text="Save">
                            <VBox.margin>
                                <Insets top="40.0" />
                            </VBox.margin>
                            <padding>
                                <Insets bottom="10.0" top="10.0" />
                            </padding>
                            <font>
                                <Font size="14.0" />
                            </font>
                            <cursor>
                                <Cursor fx:constant="HAND" />
                            </cursor>
                        </Button>
                    </children>
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                </VBox>
            </children>
            <StackPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </StackPane.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
    </padding>
    <cursor>
        <Cursor fx:constant="DEFAULT" />
    </cursor>
</StackPane>